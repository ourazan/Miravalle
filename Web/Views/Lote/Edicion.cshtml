<div id="divFormularioLote" name="divFormularioLote">
        <div class="form-group row">
            <label for="CodigoLote">Código Lote:</label>
            @Html.TextBox("CodigoLote", "", new { id = "CodigoLote", @class = "form-control", required = "", maxlength = "100" })
        </div>
        <div class="form-group row">
            <label for="FechaRegistro">Fecha Registro:</label>
            @Html.TextBox("FechaRegistro","" ,new { id = "FechaRegistro", @class = " datepicker form-control ", required = "", @readonly = "" })
        </div>
        <div class="form-group row">
            <label for="FechaVencimiento">Fecha Vencimiento:</label>
            @Html.TextBox("FechaVencimiento","" ,new { id = "FechaVencimiento", @class = " datepicker form-control ", required = "", @readonly = "" })
        </div>
        <div class="form-group row align-center">
            <input type="hidden" id="hddIDProducto" name="hddIDProducto" />
            <input type="hidden" id="hddIDLote" name="hddIDLote" />
            <button id="btnGuardar" formnovalidate class="btn btn-info" type="submit" onclick='GuardarLote(); return false;' value="">Guardar</button>
        </div>
</div>



