<div id="divFormularioInventario" name="divFormularioInventario">
    <div class="form-group row">
        <label for="Cantidad">Cantidad:</label>
        @Html.TextBox("Cantidad", "", new { id = "Cantidad", @class = "form-control", required = "", maxlength = "100" })
    </div>
    <div class="form-group row">
        <label for="FechaRegistroInventario">Fecha Registro:</label>
        @Html.TextBox("FechaRegistroInventario", "", new { id = "FechaRegistroInventario", @class = " datepicker form-control ", required = "", @readonly = "" })
    </div>
    <div class="form-group row">
        <label for="SedeInventario">Sede:</label>
        @Html.DropDownList("SedeInventario", (SelectList)ViewData["Sede"], new { id = "SedeInventario", @class = "form-control ", required = "" })
    </div>
    <div class="form-group row align-center">
        <input type="hidden" id="hddIDInventario" name="hddIDInventario" />
        <input type="hidden" id="hddIDLote" name="hddIDLote" />
        <button id="btnGuardar" formnovalidate class="btn btn-info" type="submit" onclick='GuardarInventario(); return false;' value="">Guardar</button>
    </div>
</div>




